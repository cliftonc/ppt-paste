{
  "name": "ppt-paste-server",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "download-fixtures": "node scripts/download-fixtures.js",
    "add-test-case": "node scripts/add-test-case.js",
    "log-paste": "node scripts/log-paste.js",
    "validate-samples": "node scripts/validate-samples.js"
  },
  "dependencies": {
    "fast-xml-parser": "^5.2.5",
    "jszip": "^3.10.1"
  },
  "optionalDependencies": {
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^1.6.0",
    "typescript": "^5.0.0",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
