{
  "name": "proxy-server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "dev:full": "concurrently \"npm run dev\" \"cd ../demo && pnpm dev\"",
    "build": "cd ../demo && pnpm build && cp -r dist ../proxy-server/public",
    "build:full": "pnpm --filter ppt-paste-parser build && pnpm run build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "download-fixtures": "node scripts/download-fixtures.js",
    "add-test-case": "node scripts/add-test-case.js",
    "log-paste": "node scripts/log-paste.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "pptx2json": "^0.0.10",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.6.0",
    "concurrently": "^8.2.2",
    "vitest": "^1.6.0"
  }
}
