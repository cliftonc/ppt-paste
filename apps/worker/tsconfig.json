{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "WebWorker"],
    "module": "ESNext",
    "moduleResolution": "node",
    "isolatedModules": true,
    "resolveJsonModule": true,
    "strict": false,
    "skipLibCheck": true,
    "types": ["@cloudflare/workers-types"]
  },
  "include": ["worker.ts"],
  "exclude": ["dist", "node_modules", "client"]
}
